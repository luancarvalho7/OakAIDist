{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "http://(.*)",
        "destination": "https://$1",
        "permanent": true,
        "statusCode": 301
      }
    ],
    "cleanUrls": true
  }